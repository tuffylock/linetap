<!DOCTYPE html>
<html>
<head>
  <title>LineTap</title>

  <%= render 'favicon' %>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, msg| -%>
    <%= content_tag :div, msg, class: name %>
  <% end %>

<%= yield %>

</body>
</html>
